{
  "name": "openapi-sync",
  "version": "2.0.1",
  "description": "A developer-friendly tool designed to keep your API up-to-date by leveraging OpenAPI schemas. It automates the generation of endpoint URIs and type definitions, including shared types, directly from your OpenAPI specification.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "openapi-sync": "./bin/cli.js"
  },
  "keywords": [
    "openapi",
    "rest",
    "api",
    "swagger",
    "typescript",
    "ts",
    "dts",
    "node",
    "codegen",
    "generation",
    "generator"
  ],
  "files": [
    "bin",
    "dist",
    "db.json",
    "types.ts",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\"",
    "build": "tsc",
    "publish-package": "npm run build && npm version patch &&  npm publish",
    "start": "npm run build && openapi-sync"
  },
  "author": "P-Technologies",
  "license": "ISC",
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/lodash": "^4.17.7",
    "@types/node": "^22.1.0",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@redocly/openapi-core": "^1.19.0",
    "axios": "^1.7.3",
    "axios-retry": "^4.5.0",
    "curl-generator": "^0.4.2",
    "dotenv": "^16.4.5",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "yargs": "^17.7.2"
  }
}
