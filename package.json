{
  "name": "openapi-sync",
  "version": "2.1.12",
  "description": "A developer-friendly tool designed to keep your API up-to-date by leveraging OpenAPI schemas. It automates the generation of endpoint URIs and type definitions, including shared types, directly from your OpenAPI specification.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "openapi-sync": "./bin/cli.js"
  },
  "keywords": [
    "openapi",
    "rest",
    "api",
    "swagger",
    "typescript",
    "ts",
    "dts",
    "node",
    "codegen",
    "generation",
    "generator"
  ],
  "files": [
    "bin",
    "dist",
    "db.json",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/akintomiwa-fisayo/openapi-sync"
  },
  "homepage": "https://github.com/akintomiwa-fisayo/openapi-sync#readme",
  "bugs": {
    "url": "https://github.com/akintomiwa-fisayo/openapi-sync/issues"
  },
  "scripts": {
    "build-tsc": "rm -rf dist && tsc",
    "build": "rm -rf dist && tsup",
    "publish-package": "npm run build && npm publish"
  },
  "author": "P-Technologies",
  "license": "ISC",
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/lodash": "^4.17.7",
    "@types/lodash.get": "^4.4.9",
    "@types/lodash.isequal": "^4.5.8",
    "@types/node": "^22.1.0",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@redocly/openapi-core": "^1.19.0",
    "axios": "^1.7.3",
    "axios-retry": "^4.5.0",
    "curl-generator": "^0.4.2",
    "esbuild-register": "^3.6.0",
    "js-yaml": "^4.1.0",
    "lodash.get": "^4.4.2",
    "lodash.isequal": "^4.5.0",
    "tsup": "^8.5.0",
    "yargs": "^17.7.2"
  }
}
